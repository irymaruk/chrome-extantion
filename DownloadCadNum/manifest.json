{
  "manifest_version": 3,
  "name": "DownloadCadNum",
  "description": "Download ln4 files for all casastr numbers",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": [
    "https://e.land.gov.ua/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://e.land.gov.ua/*"],
      "js": ["content-script.js"]
    }
  ]
}